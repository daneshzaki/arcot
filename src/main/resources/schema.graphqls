type Query {
  orderById(id: ID): Order 
}

type Order {
  id: ID
  date: String
  amount: String
  customer: Customer
  item: Item
}

type Customer {
  id: ID
  firstName: String
  lastName: String
  phone: String
  address: String
}


type Item {
  id: ID
  name: String
  price: String
  description: String
}
